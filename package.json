{
  "private": true,
  "name": "ssr-preact",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && sirv build --cors --single",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-synacor": "^3.0.4",
    "if-env": "^1.0.0",
    "preact-cli": "^3.0.0-rc.6",
    "sirv-cli": "^0.4.5"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "htm": "^3.0.3",
    "html": "^1.0.0",
    "ignore-styles": "^5.0.1",
    "preact": "^10.1.0",
    "preact-render-to-string": "^5.1.4"
  }
}
